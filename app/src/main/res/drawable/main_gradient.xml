<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item>
		<shape xmlns:android="http://schemas.android.com/apk/res/android"
			android:shape="rectangle">
			<gradient android:startColor="#BF994C" android:endColor="#BF994C"
				android:angle="270" />
			<padding android:left="12dp" android:top="7dp" android:right="12dp"
				android:bottom="7dp" />
			<corners android:radius="10dp" />
			<stroke android:width="0dp" android:color="#1C324D" />
		</shape>
	</item>
</selector>
