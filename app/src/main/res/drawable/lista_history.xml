<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_pressed="true">
		<shape xmlns:android="http://schemas.android.com/apk/res/android"
			android:shape="rectangle">
			<gradient android:startColor="#8F4F3F" android:endColor="#7D4C40"
				android:angle="270" />
			<padding android:left="12dp" android:top="7dp" android:right="12dp"
				android:bottom="7dp" />
			<corners android:radius="6dp" />
			<stroke android:width="1dp" android:color="#AB7061" />
		</shape>
	</item>
	<item>
		<shape xmlns:android="http://schemas.android.com/apk/res/android"
			android:shape="rectangle">
			<gradient android:startColor="#763727" android:endColor="#AB6957"
				android:angle="0" />
			<padding android:left="12dp" android:top="7dp" android:right="12dp"
				android:bottom="7dp" />
			<corners android:radius="6dp" />
			<stroke android:width="1dp" android:color="#AB7061" />
		</shape>
	</item>
</selector>
