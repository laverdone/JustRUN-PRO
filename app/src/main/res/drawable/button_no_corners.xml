<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_focused="true">
		<shape android:shape="rectangle">
			<gradient android:startColor="#56731D" android:endColor="#669900"
				android:angle="270" />
			<padding android:left="12dp" android:top="7dp" android:right="12dp"
				android:bottom="7dp" />
			<corners android:radius="0dp" />
			<stroke android:width="0.5dp" android:color="#56731D" />
		</shape>
	</item>

	<item android:state_pressed="true" >
		<shape android:shape="rectangle">
			<gradient android:startColor="#56731D" android:endColor="#669900"
				android:angle="270" />
			<padding android:left="12dp" android:top="7dp" android:right="12dp"
				android:bottom="7dp" />
			<corners android:radius="0dp" />
			<stroke android:width="0.8dp" android:color="#56731D" />
		</shape>
	</item>


	<item android:state_enabled="false">
		<shape android:shape="rectangle">
			<gradient android:startColor="#669900" android:endColor="#56731D"
				android:angle="270" />
			<padding android:left="12dp" android:top="7dp" android:right="12dp"
				android:bottom="7dp" />
			<corners android:radius="0dp" />
			<stroke android:width="0.5dp" android:color="#669900" />
		</shape>
	</item>

</selector>
