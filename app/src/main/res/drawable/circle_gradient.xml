<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_pressed="true">
		<shape xmlns:android="http://schemas.android.com/apk/res/android"
			android:shape="oval">
			<gradient android:startColor="#8F793F" android:endColor="#7D6C40"
				android:angle="0" />
			<padding android:left="12dp" android:top="7dp" android:right="12dp"
				android:bottom="7dp" />
			<corners android:radius="6dp" />
			<stroke android:width="1dp" android:color="#766027" />
		</shape>
	</item>
	<item>
		<shape xmlns:android="http://schemas.android.com/apk/res/android"
			android:shape="oval">
			<gradient android:startColor="#669900" android:endColor="#56731D"
				android:angle="270" />
			<padding android:left="12dp" android:top="7dp" android:right="12dp"
				android:bottom="7dp" />
			<corners android:radius="6dp" />
			<stroke android:width="1dp" android:color="#426300" />
		</shape>
	</item>
</selector>
